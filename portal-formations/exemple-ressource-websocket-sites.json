          "content": [
            {
              "type": "text",
              "text": "// Exemple de connexion WebSocket pour Slack\nconst ws = new WebSocket('wss://slack.com/api/rtm.connect');\n\nws.onmessage = (event) => {\n  const message = JSON.parse(event.data);\n  if (message.type === 'message') {\n    // Afficher le nouveau message instantanément\n    displayMessage(message);\n  }\n};"
            }
          ]
        },
